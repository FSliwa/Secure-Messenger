# Application
NODE_ENV=production
VITE_APP_NAME=Secure-Messenger
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://secure-messenger.your-domain.com

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Authentication URLs
VITE_REDIRECT_URL=https://secure-messenger.your-domain.com
VITE_SITE_URL=https://secure-messenger.your-domain.com

# Security
VITE_ENABLE_2FA=true
VITE_ENABLE_BIOMETRIC=true
VITE_SESSION_TIMEOUT=900000
VITE_MAX_LOGIN_ATTEMPTS=5

# Feature Flags
VITE_ENABLE_FILE_SHARING=true
VITE_ENABLE_VOICE_MESSAGES=true
VITE_MAX_FILE_SIZE=26214400
VITE_ENABLE_MESSAGE_SEARCH=true

# Analytics & Monitoring (Optional)
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=
VITE_LOG_LEVEL=error

# CDN & Assets
VITE_CDN_URL=https://cdn.your-domain.com
VITE_ASSETS_URL=https://assets.your-domain.com
